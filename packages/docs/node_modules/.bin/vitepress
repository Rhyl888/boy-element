#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/workspace/front/projects/boy-element/node_modules/.pnpm/vitepress@1.0.0-rc.44_@algolia+client-search@4.23.3_@types+node@20.12.8_postcss@8.4.38_search_7afs6pttprhdgt7dlnb56n6fte/node_modules/vitepress/bin/node_modules:/mnt/d/workspace/front/projects/boy-element/node_modules/.pnpm/vitepress@1.0.0-rc.44_@algolia+client-search@4.23.3_@types+node@20.12.8_postcss@8.4.38_search_7afs6pttprhdgt7dlnb56n6fte/node_modules/vitepress/node_modules:/mnt/d/workspace/front/projects/boy-element/node_modules/.pnpm/vitepress@1.0.0-rc.44_@algolia+client-search@4.23.3_@types+node@20.12.8_postcss@8.4.38_search_7afs6pttprhdgt7dlnb56n6fte/node_modules:/mnt/d/workspace/front/projects/boy-element/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/workspace/front/projects/boy-element/node_modules/.pnpm/vitepress@1.0.0-rc.44_@algolia+client-search@4.23.3_@types+node@20.12.8_postcss@8.4.38_search_7afs6pttprhdgt7dlnb56n6fte/node_modules/vitepress/bin/node_modules:/mnt/d/workspace/front/projects/boy-element/node_modules/.pnpm/vitepress@1.0.0-rc.44_@algolia+client-search@4.23.3_@types+node@20.12.8_postcss@8.4.38_search_7afs6pttprhdgt7dlnb56n6fte/node_modules/vitepress/node_modules:/mnt/d/workspace/front/projects/boy-element/node_modules/.pnpm/vitepress@1.0.0-rc.44_@algolia+client-search@4.23.3_@types+node@20.12.8_postcss@8.4.38_search_7afs6pttprhdgt7dlnb56n6fte/node_modules:/mnt/d/workspace/front/projects/boy-element/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
